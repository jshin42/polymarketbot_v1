{
  "name": "@polymarketbot/collector",
  "version": "1.0.0",
  "description": "Data collection service for Polymarket markets, order books, and trades",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:integration": "vitest run --config vitest.integration.config.ts",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@polymarketbot/shared": "workspace:*",
    "bullmq": "^5.1.0",
    "ioredis": "^5.4.1",
    "pg": "^8.12.0",
    "pino": "^8.17.2",
    "ws": "^8.16.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@types/pg": "^8.11.6",
    "@types/ws": "^8.5.10",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0"
  }
}
